<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androiddev.artemqa.gototrip">

    <application
        android:allowBackup="true"
        android:name=".common.AppGoToTrip"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".modules.login.view.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".modules.register.view.RegisterActivity" />
        <activity android:name=".modules.main.view.MainActivity" />
        <activity android:name=".editProfile.view.EditProfileActivity"></activity>
    </application>

</manifest>